programa
{
	
	funcao inicio()
	{
		inteiro num1, num2
		
		escreva("Digite o primeiro número : \n")
		leia(num1)
		escreva("Digite o segundo número : \n")
		leia(num2)

		se (num1 > num2){
			escreva("O número ",num1," é maior ")
		}senao{
			escreva("O número ",num2," é maior")
		}
		
	}
}
